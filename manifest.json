{
    "name": "今日安全",
    "version": "1.35",
    "description": "看今日安全，漏洞全知道",
    "permissions": ["tabs", "declarativeContent", "https://*/"],
    "chrome_url_overrides" : {
        "newtab": "myPage.html"
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "page_action": {
        "default_icon": {
            "16": "images/SAI16.png",
            "32": "images/SAI32.png",
            "48": "images/SAI48.png",
            "128": "images/SAI128.png"
        }
    },
    "icons": {
        "16": "images/SAI16.png",
        "32": "images/SAI32.png",
        "48": "images/SAI48.png",
        "128": "images/SAI128.png"
    },
    "manifest_version": 2
}